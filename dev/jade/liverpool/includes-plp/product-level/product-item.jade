block var3
  - var canceled = false
block main_class
 .product.item
  if clickable && canceled
   .first_item-module.product_item.canceled.clickable
     include ./product-content.jade
  else if clickable
   .first_item-module.product_item.clickable
     include ./product-content.jade
  else if priced
   .first_item-module.product_item.ebook
     include ./product-content.jade
  else
   .first_item-module.product_item.noclickable
     include ./product-content.jade

