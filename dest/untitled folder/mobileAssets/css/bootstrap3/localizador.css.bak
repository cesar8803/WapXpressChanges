.input-filter{
  border:0!important;
}
.modal-filter {
  position: fixed;
  top: 103px;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1040;
  display: none;
  overflow: hidden;
  -webkit-overflow-scrolling: touch;
  outline: 0;
}
.modal-dialog-filter {
  width: 16em;
  height: 100%;
  float: right;
  margin-top: 0px;
    margin: 0;
}
.modal-content-filter {
  border-radius: 0px;
  height: 100%;
  width: 100%;
}

.radio-filter label{
    min-height: 45px;
    font-size: 1.3em;
}

