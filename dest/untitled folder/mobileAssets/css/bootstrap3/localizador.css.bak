html { width:100%;height: 100%; }
body {
  width:100%;
   height: 100%;
   margin: 0!important;
   padding: 0!important;
}



.orientleft #shell {
    -webkit-transform: rotate(-90deg);
    -webkit-transform-origin:160px 160px;
}
 
.orientright #shell {
    -webkit-transform: rotate(90deg);
    -webkit-transform-origin:230px 230px;
} 
.container { 
  width:100%;
  height: 100%;
  padding:0!important;
  margin:0!important;
}
.gl-map { 
  position: fixed;
  width:100%;
  height: 90%;
    margin: 0;
  padding: 0;
z-index: 0;
   }
.header{height: 10%;}
#btn-all{
position: absolute;
width: 100%;
height: 6%;
bottom:0%;
left: 0em;
z-index: 4;
}

#btn-all button{
  background:#cf0078;
  border-radius: 0;
  border: 0;
}
.dropdown-menu{
  width: 100%;
}
.dropdown-menu li{

}
.dropdown-menu li .active{
background:#2fbae5;
}

.dropdown-menu > .active > a, .dropdown-menu > .active > a:hover, .dropdown-menu > .active > a:focus{
  background-color:#2fbae5!important;
    background-image: -webkit-linear-gradient(top, #2fbae5 0%, #2fbae5 100%)!important; 
  background-image: -o-linear-gradient(top, #2fbae5 0%, #2fbae5 100%)!important;
   background-image: -webkit-gradient(linear, left top, left bottom, from(#2fbae5), to(#2fbae5))!important; 
  background-image: linear-gradient(to bottom, #2fbae5 0%, #2fbae5 100%)!important; 
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='##2fbae5', endColorstr='#ff2e6da4', GradientType=0);
  background-repeat: repeat-x;

}

.dropdown-menu li a{
  color:#646464;
}

