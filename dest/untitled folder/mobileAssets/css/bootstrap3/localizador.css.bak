.modal-by-estate {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1040;
  display: none;
  overflow: hidden;
  -webkit-overflow-scrolling: touch;
  outline: 0;
}
.modal-dialog-estate{
  width: 16em;
  height: 100%;
  float: right;
  margin-top: 0px;
    margin: 0;
}
.modal-content-estate {
  border-radius: 0px;
  height: 100%;
  width: 100%;
  overflow-y:auto; 
}
.radio-estate label{
    min-height: 45px;
    font-size: 1.3em;
}
.modal-backdrop.in{
  z-index:1;
    width: 0;
  height: 0!important;
  display:none;
}
.modal.in .modal-dialog {
  z-index:9;
}
